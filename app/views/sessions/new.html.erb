<div class="login">
	<div class="container">
		<div class="form">

      <h1>Log in</h1>
			<%= form_for :session, url: login_path do |f| %> 
                <%= f.label :email %>
                <%= f.text_field :email %>
                <%= f.label :password %>
                <%= f.password_field :password %>
                <%= f.submit "Login" %>
            <% end %>

		
    </div>
	</div>
</div>